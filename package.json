{
  "name": "art-list-generator",
  "version": "1.0.0",
  "description": "Redux practice",
  "main": "main.js",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "karma start",
    "bundle": "NODE_ENV=development webpack --mode development",
    "bundle-dist": "NODE_ENV=production webpack --mode production --config webpack.config.prod.js",
    "dist": "NODE_ENV=production electron-builder",
    "build": "npm-run-all bundle-dist dist",
    "serve": "electron .",
    "start-electron": "npm-run-all --parallel bundle serve",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . art-list-generator --platform=darwin --out ../OutApp/mac --electronVersion=6.0.4 --overwrite --icon=./app.icns --ignore='./.git(ignore|modules)'",
    "package:win": "electron-packager . art-list-generator --platform=win32 --out ../OutApp/win --electronVersion=6.0.4 --overwrite --icon=./app.ico --ignore='./.git(ignore|modules)'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justin3737/redux-message-board.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^7.1.5",
    "css-loader": "^0.28.10",
    "electron": "^6.0.4",
    "electron-builder": "^20.13.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.0.4",
    "electron-reload": "^1.2.2",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.13.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.10",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "0.1.1",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^2.3.4",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^3.1.0",
    "react-hot-loader": "^1.3.0",
    "react-svg-loader": "^3.0.3",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "antd": "^3.22.1",
    "babel-core": "^7.0.0-bridge.0",
    "electron-is-dev": "^0.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.9.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "classnames": "^2.2.3",
    "history": "^1.17.0",
    "jquery": "^2.1.4",
    "react-addons-test-utils": "0.14.4",
    "redux-simple-router": "^2.0.4"
  }
}
